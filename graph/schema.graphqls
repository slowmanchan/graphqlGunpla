# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Gunpla {
  id: ID!
  boxArtImageLink: String!
  title: String!
	subtitle: String!
	classification: String!
	lineupNo: String!
	scale: String!
	franchise: String!
	releaseDate: String!
	janIsbn: String!
	run: String!
	price: String!
  includes: String!
  features: String!
}

type Query {
  gunplas: [Gunpla!]!
}